





generator client {
  provider = "prisma-client"
  output = "./generated"
}

datasource db {
  provider = "postgresql"
}

model DefenseSession {
  id String @id @default(uuid()) 
  title String
  codeSnippet String @db.Text
  score     Int?
  questions  String
  login String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
